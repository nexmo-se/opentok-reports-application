    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "fecha": "^4.2.0",
    "graphql": "^15.0.0",
    "moment": "^2.25.3",
    "morgan": "~1.9.0",
    "objects-to-csv": "^1.3.6",
